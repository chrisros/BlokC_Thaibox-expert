@using Webshop2.Models
@model List<Product>

@{
    ViewBag.Title = "ProductWijzigen";
}

<h1>@ViewBag.H1</h1>

@{foreach (Product prod in ViewBag.productLijstje)
{
    <div>
        <b>Naam: </b> @prod.productNaam <br />
        <b>Merk: </b> @prod.productMerk <br />
        <b>Prijs: </b> @prod.productPrijs <br />
        <a href="/Store/wijzigenProduct?prodID=@prod.productID">Wijzigen</a> <br /><br />
    </div>
}}
