@using Webshop2.Models;
@model List<Product>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Het artikel is toegevoegd aan het winkelmandje!</title>
    <h2>Het artikel is toegevoegd aan uw winkelmandje</h2>
</head>
<body>
    <div> 
        @{foreach(Product p in Model)
        {
            @p.productNaam
            <form action="~/Cart/"><input type="submit" value="Ga naar winkelmandje" /></form>
        }}
      
    </div>
    
</body>
</html>
